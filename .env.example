
# API Configuration
VITE_API_BASE_URL=https://localhost:7001
VITE_APP_ENV=development

# Supabase Configuration (for user sync and inventory management)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# SQL Server Configuration (for direct user sync from AspNetUser table)
# These are required if you want to sync directly from SQL Server instead of CSV
SQL_SERVER_HOST=your_sql_server_host
SQL_SERVER_USER=your_sql_server_username
SQL_SERVER_PASSWORD=your_sql_server_password
SQL_SERVER_DATABASE=your_database_name

# Examples:
# SQL_SERVER_HOST=192.168.1.100
# SQL_SERVER_HOST=sql-server.company.com
# SQL_SERVER_USER=inventory_user
# SQL_SERVER_PASSWORD=your_secure_password
# SQL_SERVER_DATABASE=ECPInventoryDB

# Optional: Add other environment variables as needed
# VITE_APP_NAME=Inventory Management System
# VITE_APP_VERSION=1.0.0
